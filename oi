--includedir=/usr/include
--libdir=/usr/lib64
--libexecdir=/usr/lib64
--prefix=%{_prefix}
--target=x86_64-generic-linux
--build=x86_64-generic-linux
--disable-bootstrap
--disable-cet
--disable-default-pie
--disable-libmpx
--disable-multiarch
--disable-multilib
--disable-vtable-verify
